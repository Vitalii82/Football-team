<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.football.mybatis.mapper.SeasonMapper">
  <select id="findAll" resultType="com.football.model.Season">
    SELECT * FROM seasons
  </select>

  <insert id="insert" parameterType="com.football.model.Season">
    INSERT INTO seasons (start_year, end_year, name)
    VALUES (#{startYear}, #{endYear}, #{name})
  </insert>
</mapper>
