<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.football.mybatis.mapper.TeamMapper">
  <select id="findAll" resultType="com.football.model.Team">
    SELECT * FROM teams
  </select>

  <insert id="insert" parameterType="com.football.model.Team">
    INSERT INTO teams (team_name, founded_year, stadium_id, manager_id)
    VALUES (#{teamName}, #{foundedYear}, #{stadiumId}, #{managerId})
  </insert>
</mapper>
